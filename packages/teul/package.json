{
  "name": "teul",
  "description": "teul(í‹€) The minimal React framework",
  "version": "0.0.1-alpha.1",
  "type": "module",
  "author": "Yuno Seo",
  "keywords": [
    "react",
    "minimal",
    "framework",
    "teul"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yunho1017/yuno.dev.git",
    "directory": "packages/teul"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/main.d.ts",
      "react-server": "./dist/main.react-server.js",
      "default": "./dist/main.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./router/server": {
      "types": "./dist/router-server.d.ts",
      "default": "./dist/router-server.js"
    },
    "./router/client": {
      "types": "./dist/router-client.d.ts",
      "default": "./dist/router-client.js"
    },
    "./adapters/*": {
      "types": "./dist/adapters/*.d.ts",
      "default": "./dist/adapters/*.js"
    },
    "./internals": {
      "types": "./dist/internals.d.ts",
      "default": "./dist/internals.js"
    }
  },
  "bin": {
    "teul": "./cli.js"
  },
  "files": [
    "dist",
    "cli.js"
  ],
  "scripts": {
    "dev": "swc src -d dist -w --strip-leading-paths",
    "test": "vitest run",
    "test:watch": "vitest",
    "compile": "rm -rf dist *.tsbuildinfo && pnpm run compile:code && pnpm run compile:types && cp ../../README.md .",
    "compile:code": "swc src -d dist --strip-leading-paths",
    "compile:types": "tsc --project tsconfig.json"
  },
  "license": "MIT",
  "engines": {
    "node": "^24.0.0 || ^22.12.0 || ^20.19.0"
  },
  "dependencies": {
    "@hono/node-server": "1.19.1",
    "@swc/core": "1.13.5",
    "@vitejs/plugin-react": "5.0.2",
    "@vitejs/plugin-rsc": "0.4.29",
    "dotenv": "17.2.2",
    "hono": "4.9.6",
    "rsc-html-stream": "0.0.7",
    "vite": "7.1.5",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@netlify/functions": "^4.2.5",
    "@swc/cli": "^0.7.8",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-server-dom-webpack": "19.1.1",
    "ts-expect": "^1.3.0",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "~19.1.1",
    "react-dom": "~19.1.1",
    "react-server-dom-webpack": "~19.1.1"
  }
}
